<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>view seller</title>
    <link rel="stylesheet" href="../../styles.css" />
  </head>
  <body>
    <%-include ('adminMenu')%>
    <div class="card">
      <% result.forEach(result=>{%>
      <div class="car">
        <h3>Seller name: <%= result.first_name%> <%= result.last_name%></h3>
        <img
          src="../../<%= result.identity_card%>"
          alt=""
          width="100"
          height="100"
        />
        <p>shop number <%= result.shop_number%></p>
        <p>Phone number <%= result.phone_number%></p>
        <p>address <%= result.address%></p>
        <p>Seller id <%= result.seller_id%></p>

        <div>
          <a
            href="/admin/deleteSeller/<%= result.seller_id%> "
            class="btndelete"
            >Delete</a
          >
        </div>
      </div>
      <%})%>
    </div>

    <%- include('../footer')%>
  </body>
</html>
