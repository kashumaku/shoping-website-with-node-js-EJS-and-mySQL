<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>view delivery man</title>
    <link rel="stylesheet" href="../../styles.css" />
  </head>
  <body>
    <%-include ('adminMenu')%>
    <div class="card">
      <% result.forEach(result=>{%>
      <div class="car">
        <h3>
          Delivery man name: <%= result.first_name%> <%= result.last_name%>
        </h3>

        <p>Phone number: <%= result.phone_number%></p>
        <p>Address: <%= result.address%></p>
        <p>Delivery man id: <%= result.delivery_id%></p>
        <p>Registration date: <%= result.dat%></p>
        <div class="btn">
          <a href="/admin/deleteDeliveryMan/<%= result.delivery_id%>">Delete</a>
        </div>
      </div>
      <%})%>
    </div>

    <%- include('../footer')%>
  </body>
</html>
